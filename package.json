{
  "name": "clientroomradio",
  "description": "A collaborative radio experience for hearts and minds",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "body-parser": "1.3.1",
    "request": "2.27.0",
    "lastfm": "0.9.2",
    "vlc": "https://github.com/clientroomradio/node-vlc/tarball/master",
    "spotify-web": "https://github.com/Shigawire/node-spotify-web/tarball/master",
    "winston": "0.7.2",
    "express": "4.2.0",
    "cookie-parser": "1.1.0",
    "sockjs": "0.3.7",
    "node-uuid": "1.4.1",
    "http-proxy": "0.10.3",
    "argparse": "1.0.6",
    "cli-interact": "0.1.9"
  },
  "devDependencies": {
    "mocha": "2.4.5",
    "mocha-phantomjs": "4.0.2",
    "eslint": "2.3.0",
    "chai": "3.5.0"
  },
  "scripts": {
    "test": "npm run-script test-lint && npm run-script test-node && npm run-script test-browser",
    "test-lint": "eslint .",
    "test-node": "mocha",
    "test-browser": "mocha-phantomjs ./static/test.html"
  },
  "bin": {
    "crr-run": "./bin/app.js",
    "crr-setup": "./bin/setup.js"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true,
      "es6": true
    },
    "rules": {
      "indent": [2, 4],
      "quotes": [2, "double"],
      "semi": [2, "always"]
    }
  }
}
