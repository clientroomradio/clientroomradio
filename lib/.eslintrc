{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "node": true,
        "browser": false
    },
    "globals": {
        "_": flase
    },
    "rules": {
        "no-console": false,
        "strict": 0
    }
}